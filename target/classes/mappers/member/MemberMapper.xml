<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itbank.navercafe.mybatis.member.MemberMapper">

	<resultMap type="com.itbank.navercafe.user.member.dto.MemberDTO" id="project_members">
		<id property="userId" column="user_id"/>
		<result property="userPw" column="user_pw"/>
		<result property="userName" column="user_name"/>
		<result property="userPhone" column="user_phone"/>
		<result property="userEmail" column="user_email"/>
		<result property="postalCode" column="postalCode"/>
		<result property="roadAddr" column="roadAddr"/>
		<result property="jibunAddr" column="jibunAddr"/>
		<result property="certAddr" column="certAddr"/>
		<result property="regDate" column="regDate"/>
		<result property="userPoint" column="user_point"/>
		<result property="userLevel" column="user_level"/>
	</resultMap>
	
	<select id="getMemberList" resultMap="project_members">
		select * from project_members
	</select>
	
	
	
	
	
	
	
</mapper>